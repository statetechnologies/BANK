<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="509.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bank.bank.NMBController">
   <children>
      <Label alignment="CENTER" layoutX="226.0" layoutY="118.0" prefHeight="27.0" prefWidth="130.0" text="NMB BANK" textFill="#5d2aa1" underline="true">
         <font>
            <Font name="Lucida Grande" size="21.0" />
         </font>
         <effect>
            <Reflection bottomOpacity="0.32" fraction="0.6" topOpacity="0.27" />
         </effect>
      </Label>
      <Button fx:id="home" layoutX="7.0" layoutY="3.0" mnemonicParsing="false" style="-fx-background-color: transparent;" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <graphic>
            <ImageView fitHeight="23.0" fitWidth="23.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@home.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <HBox layoutX="20.0" layoutY="201.0" prefHeight="27.0" prefWidth="561.0" spacing="20.0">
         <children>
            <TextField fx:id="firstname" promptText="First name">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <TextField fx:id="middlename" promptText="Middle name">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <TextField fx:id="surname" promptText="Surname">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
         </children>
      </HBox>
      <HBox layoutX="20.0" layoutY="228.0" prefHeight="27.0" prefWidth="561.0" spacing="20.0">
         <children>
            <TextField fx:id="email" prefHeight="25.0" prefWidth="378.0" promptText="E-mail">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <ComboBox fx:id="sex" prefHeight="25.0" prefWidth="87.0" promptText="Sex">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ComboBox>
         </children>
      </HBox>
      <ImageView fitHeight="104.0" fitWidth="213.0" layoutX="195.0" layoutY="3.0" pickOnBounds="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <image>
            <Image url="@tz-nmb-logo.png" />
         </image>
      </ImageView>
      <HBox layoutX="20.0" layoutY="255.0" prefHeight="27.0" prefWidth="561.0" spacing="20.0">
         <children>
            <TextField fx:id="account_no" prefHeight="25.0" prefWidth="241.0" promptText="Account No.">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <TextField fx:id="card_no" prefHeight="25.0" prefWidth="269.0" promptText="Card No.">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <PasswordField fx:id="pin" prefHeight="25.0" prefWidth="96.0" promptText="PIN" />
         </children>
      </HBox>
      <HBox layoutX="20.0" layoutY="282.0" prefHeight="27.0" prefWidth="561.0" spacing="20.0">
         <children>
            <TextField fx:id="City" prefHeight="25.0" prefWidth="241.0" promptText="City">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
            <ComboBox fx:id="account_type" prefHeight="25.0" prefWidth="125.0" promptText="Account Type">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ComboBox>
         </children>
      </HBox>
      <HBox layoutX="20.0" layoutY="309.0" prefHeight="27.0" prefWidth="561.0" spacing="20.0">
         <children>
            <TextField fx:id="phone" promptText="Phone Number">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TextField>
         </children>
      </HBox>
      <Button fx:id="register" layoutX="267.0" layoutY="466.0" mnemonicParsing="false" style="-fx-background-color: #5d2aa1#5d2aa1;" text="Register">
         <font>
            <Font name="Lucida Grande" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <graphic>
            <ImageView fitHeight="32.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@tickmark.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="203.0" layoutY="169.0" text="Fill in the required details in this form" underline="true" />
   </children>
</AnchorPane>
